{
  "name": "ngembed",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "rimraf dist/xpr/embed",
    "build": "ng build --prod --project @xpr/embed",
    "test": "ng test --project @xpr/embed",
    "lint": "ng lint",
    "publish": "cd dist/xpr/embed && npm publish --access public",
    "postinstall": "ngcc",
    "perbuild:embedded": "rimraf dist/embedded",
    "build:embedded": "ng build --project embedded --prod --output-hashing=none --single-bundle",
    "predemo": "npm run build:embedded && cpx \"dist/embedded/*\" projects/demo/src/assets",
    "demo": "ng serve demo"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/elements": "^10.0.14",
    "@angular/forms": "~10.0.9",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "@xpr/embed": "0.0.1",
    "cpx": "^1.5.0",
    "document-register-element": "^1.7.2",
    "ngx-build-plus": "^10.1.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
