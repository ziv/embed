{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@xpr/ngembed": {
      "projectType": "library",
      "root": "projects/xpr/ngembed",
      "sourceRoot": "projects/xpr/ngembed/src",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/xpr/ngembed/tsconfig.lib.json",
            "project": "projects/xpr/ngembed/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/xpr/ngembed/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/xpr/ngembed/src/test.ts",
            "tsConfig": "projects/xpr/ngembed/tsconfig.spec.json",
            "karmaConfig": "projects/xpr/ngembed/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/xpr/ngembed/tsconfig.lib.json",
              "projects/xpr/ngembed/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demo": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/demo",
      "sourceRoot": "projects/demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo",
            "index": "projects/demo/src/index.html",
            "main": "projects/demo/src/main.ts",
            "polyfills": "projects/demo/src/polyfills.ts",
            "tsConfig": "projects/demo/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/demo/src/favicon.ico",
              "projects/demo/src/assets"
            ],
            "styles": [
              "projects/demo/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo/src/environments/environment.ts",
                  "with": "projects/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo/src/test.ts",
            "polyfills": "projects/demo/src/polyfills.ts",
            "tsConfig": "projects/demo/tsconfig.spec.json",
            "karmaConfig": "projects/demo/karma.conf.js",
            "assets": [
              "projects/demo/src/favicon.ico",
              "projects/demo/src/assets"
            ],
            "styles": [
              "projects/demo/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/demo/tsconfig.app.json",
              "projects/demo/tsconfig.spec.json",
              "projects/demo/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/demo/e2e/protractor.conf.js",
            "devServerTarget": "demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo:serve:production"
            }
          }
        }
      }
    },
    "embedded": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/embedded",
      "sourceRoot": "projects/embedded/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/embedded",
            "index": "projects/embedded/src/index.html",
            "main": "projects/embedded/src/main.ts",
            "polyfills": "projects/embedded/src/polyfills.ts",
            "tsConfig": "projects/embedded/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/embedded/src/favicon.ico",
              "projects/embedded/src/assets"
            ],
            "styles": [
              "projects/embedded/src/styles.css"
            ],
            "scripts": [
              {
                "bundleName": "polyfill-webcomp",
                "input": "node_modules/@webcomponents/webcomponentsjs/bundles/webcomponents-sd-ce-pf.js"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/embedded/src/environments/environment.ts",
                  "with": "projects/embedded/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "embedded:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "embedded:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "embedded:build"
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "projects/embedded/src/test.ts",
            "polyfills": "projects/embedded/src/polyfills.ts",
            "tsConfig": "projects/embedded/tsconfig.spec.json",
            "karmaConfig": "projects/embedded/karma.conf.js",
            "assets": [
              "projects/embedded/src/favicon.ico",
              "projects/embedded/src/assets"
            ],
            "styles": [
              "projects/embedded/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/embedded/tsconfig.app.json",
              "projects/embedded/tsconfig.spec.json",
              "projects/embedded/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/embedded/e2e/protractor.conf.js",
            "devServerTarget": "embedded:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "embedded:serve:production"
            }
          }
        }
      }
    },
    "embed": {
      "projectType": "library",
      "root": "projects/embed",
      "sourceRoot": "projects/embed/src",
      "prefix": "xpr",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/embed/tsconfig.lib.json",
            "project": "projects/embed/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/embed/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/embed/src/test.ts",
            "tsConfig": "projects/embed/tsconfig.spec.json",
            "karmaConfig": "projects/embed/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/embed/tsconfig.lib.json",
              "projects/embed/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@xpr/embed": {
      "projectType": "library",
      "root": "projects/xpr/embed",
      "sourceRoot": "projects/xpr/embed/src",
      "prefix": "xpr",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/xpr/embed/tsconfig.lib.json",
            "project": "projects/xpr/embed/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/xpr/embed/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/xpr/embed/src/test.ts",
            "tsConfig": "projects/xpr/embed/tsconfig.spec.json",
            "karmaConfig": "projects/xpr/embed/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/xpr/embed/tsconfig.lib.json",
              "projects/xpr/embed/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "@xpr/ngembed"
}
